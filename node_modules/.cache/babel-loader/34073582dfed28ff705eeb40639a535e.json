for=\"lang in languages\"\n              :key=\"lang.index\"\n              :id=\"lang.index\"\n            >\n              {{ lang.name }}\n            </button>\n          </div> -->\n          <!-- {{projects}} -->\n          <div class=\"col\">\n            <button class=\"btn btn-success\" @click=\"setEngl()\">English</button>\n            | <button class=\"btn btn-success\" @click=\"setFr()\">French</button> |\n            <button class=\"btn btn-success\" @click=\"setCh()\">Chinese</button> |\n            <button class=\"btn btn-success\" @click=\"setSp()\">Spanish</button> |\n            <button class=\"btn btn-success\" @click=\"setRu()\">Russian</button> |\n          </div>\n        </div>\n\n        <div class=\"container\" v-if=\"en == true\">\n          <div v-for=\"project in projects\" :key=\"project.identifier\">\n            <p v-show=\"project.in_language.name == 'English'\">\n              <li>{{project.name}}</li>\n              <li>{{project.identifier}}</li>\n              <li>{{project.url}}</li>{{ project }}</p>\n          </div>\n        </div>\n\n        <div class=\"container\" v-if=\"fr == true\">\n          <div v-for=\"project in projects\" :key=\"project.identifier\">\n            <p v-show=\"project.in_language.name == 'French'\">{{ project }}</p>\n          </div>\n        </div>\n\n        <div class=\"container\" v-if=\"sp == true\">\n          <div v-for=\"project in projects\" :key=\"project.identifier\">\n            <p v-show=\"project.in_language.name == 'Spanish'\">{{ project }}</p>\n          </div>\n        </div>\n\n        <div class=\"container\" v-if=\"ch == true\">\n          <div v-for=\"project in projects\" :key=\"project.identifier\">\n            <p v-show=\"project.in_language.name == 'Chinese'\">{{ project }}</p>\n          </div>\n        </div>\n\n        <div class=\"container\" v-if=\"ru == true\">\n          <div v-for=\"project in projects\" :key=\"project.identifier\">\n            <p v-show=\"project.in_language.name == 'Russian'\">{{ project }}</p>\n          </div>\n        </div>\n        <!-- <div class=\"row\" v-if=\"en == true\">\n          \n          <ul v-for=\"project in projects\" :key=\"project.identifier\">\n            <li v-show=\"(project.in_languages = 'English')\">{{ project }}</li>\n          </ul>\n        </div>\n\n        <div class=\"row\" v-if=\"fr == true\">\n           \n          <ul v-for=\"project in projects\" :key=\"project.identifier\">\n            <li v-show=\"(project.in_languages = 'French')\">{{ project }}</li>\n          </ul>\n        </div>\n\n        <div class=\"row\" v-if=\"sp == true\">\n          <ul v-for=\"project in projects\" :key=\"project.identifier\">\n            <li v-show=\"(project.in_languages = 'Spanish')\">{{ project }}</li>\n          </ul>\n        </div>\n\n        <div class=\"row\" v-if=\"ch == true\">\n          <ul v-for=\"project in projects\" :key=\"project.identifier\">\n            <li v-show=\"(project.in_languages = 'Chinese')\">{{ project }}</li>\n          </ul>\n        </div>\n\n        <div class=\"row\" v-if=\"ru == true\">\n          <ul v-for=\"project in projects\" :key=\"project.identifier\">\n            <li v-show=\"(project.in_languages = 'Russian')\">{{ project }}</li>\n          </ul>\n        </div> -->\n        <!-- <ul v-for=\"project in projects\" :key=\"project.identifier\">\n          <p v-if=\"en==true\">\n          <li>{{project.in_language.name==\"English\"}}</li>\n        </p> -->\n        <!-- <li v-if=\"(project.in_language.name = `${languages.name}`)\">{{ project }}</li> -->\n        <!-- </ul> -->\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\n// import axios from \"axios\";\n// @ is an alias to /src\n// import Login from \"@/components/Login.vue\"\nimport axios from \"axios\";\nimport NavBar from \"@/components/NavBar.vue\";\nexport default {\n  name: \"HomeView\",\n  components: {\n    NavBar,\n  },\n  data() {\n    return {\n      projects: [],\n      en: false,\n      fr: false,\n      sp: false,\n      ch: false,\n      ru: false,\n    };\n  },\n  mounted() {\n    this.getProjects();\n    // this.getLanguages();\n    this.setEngl();\n    this.setFr();\n    this.setSp();\n    this.setCh();\n    this.setRu();\n  },\n  computed() {},\n\n  methods: {\n    async getProjects() {\n      const res = await axios.get(\n        \"https://api.enterprise.wikimedia.com/v1/projects\"\n      );\n      this.projects = res.data;\n      console.log(typeof res);\n    },\n    setEngl() {\n      (this.fr = false),\n        (this.sp = false),\n        (this.ch = false),\n        (this.ru = false);\n      this.en = true;\n      console.log(\"English\");\n    },\n    setFr() {\n      (this.ru = false),\n        (this.ch = false),\n        (this.sp = false),\n        (this.en = false);\n      this.fr = true;\n      console.log(\"French\");\n    },\n    setCh() {\n      (this.ru = false),\n        (this.sp = false),\n        (this.fr = false),\n        (this.en = false);\n      this.ch = true;\n      console.log(\"Chinese\");\n    },\n    setSp() {\n      (this.ru = false),\n        (this.ch = false),\n        (this.fr = false),\n        (this.en = false);\n      this.sp = true;\n      console.log(\"Spanish\");\n    },\n    setRu() {\n      (this.ch = false),\n        (this.sp = false),\n        (this.fr = false),\n        (this.en = false);\n      (this.ru = true), console.log(\"Russian\");\n    },\n  },\n};\n</script>\n<style scoped>\n.btn-success {\n  margin: 0.2%;\n}\n</style>"]},"metadata":{},"sourceType":"module"}