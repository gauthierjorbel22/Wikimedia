 is an alias to /src\n// import Login from \"@/components/Login.vue\"\nimport axios from \"axios\";\nimport NavBar from \"@/components/NavBar.vue\";\nexport default {\n  name: \"HomeView\",\n  components: {\n    NavBar,\n    NavBar,\n  },\n  data() {\n    return {\n      projects: [],\n      en: false,\n      fr: false,\n      sp: false,\n      ch: false,\n      sw: false,\n    };\n  },\n  mounted() {\n    this.getProjects();\n    // this.getLanguages();\n    this.setEngl()\n  },\n  computed(){\n \n  },\n\n  methods: {\n    async getProjects() {\n      const res = await axios.get(\n        \"https://api.enterprise.wikimedia.com/v1/projects\"\n      );\n      this.projects = res.data;\n      console.log(typeof res);\n    },\n    // getLanguages() {},\n    // filterByLang() {\n    //   this.ch = true;\n    // },\n\n    setEngl() {\n      this.en = true;\n      console.log(\"English\")\n    },\n    setFr() {\n      this.fr = true\n    },\n    setCh() {\n      this.sp = true\n    },\n    setSp() {\n      this.ch = true\n    },\n  },\n};\n</script>\n<style scoped>\n.btn-success {\n  margin: 0.2%;\n}\n</style>"]},"metadata":{},"sourceType":"module"}