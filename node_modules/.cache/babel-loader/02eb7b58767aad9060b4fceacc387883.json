{"ast":null,"code":"// import axios from \"axios\";\n// @ is an alias to /src\n// import Login from \"@/components/Login.vue\"\nimport axios from \"axios\";\nimport NavBar from \"@/components/NavBar.vue\";\nexport default {\n  name: \"HomeView\",\n  components: {\n    NavBar,\n    NavBar\n  },\n\n  data() {\n    return {\n      projects: [],\n      en: false,\n      fr: false,\n      sp: false,\n      ch: false,\n      ru: false\n    };\n  },\n\n  mounted() {\n    this.getProjects();\n    this.setEngl();\n    this.setFr();\n    this.setSp();\n    this.setCh(); // this.setRu();\n  },\n\n  computed() {\n    setEngl();\n  },\n\n  methods: {\n    async getProjects() {\n      const res = await axios.get(\"https://api.enterprise.wikimedia.com/v1/projects\");\n      this.projects = res.data;\n      console.log(typeof res);\n    },\n\n    setEngl() {\n      this.en = true;\n      this.fr, this.sp, this.ch, this.ru = false;\n      console.log(\"English\");\n    },\n\n    setFr() {\n      this.fr = true;\n      this.ru, this.ch, this.sp, this.en = false;\n      console.log(\"French\");\n    },\n\n    setCh() {\n      this.ch = true;\n      this.ru, this.sp, this.fr, this.en = false;\n      console.log(\"Chinese\");\n    },\n\n    setSp() {\n      this.sp = true;\n      this.ru, this.ch, this.fr, this.en = false;\n      console.log(\"Spanish\");\n    },\n\n    setRu() {\n      this.ru = true, this.ch, this.sp, this.fr, this.en = false;\n      console.log(\"Russian\");\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqDA;AACA;AACA;AACA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,MAAP,MAAmB,yBAAnB;AACA,eAAe;EACbC,IAAI,EAAE,UADO;EAEbC,UAAU,EAAE;IACVF,MADU;IAEVA;EAFU,CAFC;;EAMbG,IAAI,GAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EADL;MAELC,EAAE,EAAE,KAFC;MAGLC,EAAE,EAAE,KAHC;MAILC,EAAE,EAAE,KAJC;MAKLC,EAAE,EAAE,KALC;MAMLC,EAAE,EAAE;IANC,CAAP;EAQD,CAfY;;EAgBbC,OAAO,GAAG;IACR,KAAKC,WAAL;IACA,KAAKC,OAAL;IACA,KAAKC,KAAL;IACA,KAAKC,KAAL;IACA,KAAKC,KAAL,GALQ,CAMR;EACD,CAvBY;;EAwBbC,QAAQ,GAAG;IACTJ,OAAO;EACR,